pipeline {
    agent 'any'

    stages {
        stage ('Clone') {
            steps {
            echo "Download repo"
            }
        }
        stage ('Build') {
            steps {
            echo "Build Maven job"
            }
        }
        stage ('Deploy') {
            steps {
            echo "Deploy Maven job"
            }
        }
        stage ('Test') {
            steps {
            echo "Test Maven job"
            }
        }
        stage ('Release') {
            steps {
            echo "Release Maven Job"
            }
        }
    }
}
